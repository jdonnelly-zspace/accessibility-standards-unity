{
  "name": "accessibility-standards",
  "version": "1.0.0",
  "description": "Comprehensive accessibility standards framework for WCAG 2.2 Level AA compliance across all projects",
  "main": "index.js",
  "type": "module",
  "bin": {
    "a11y-setup": "./bin/setup.js"
  },
  "scripts": {
    "setup": "node bin/setup.js",
    "scrape:update": "node scrapers/update-standards.js",
    "validate:html": "./scripts/validate-html.sh",
    "validate:css": "./scripts/validate-css.sh",
    "check:links": "./scripts/check-links.sh",
    "validate:all": "npm run validate:html && npm run validate:css",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "accessibility",
    "wcag",
    "a11y",
    "section-508",
    "aria",
    "vpat",
    "wcag-2.2",
    "level-aa",
    "testing",
    "standards"
  ],
  "author": "jPdonnelly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdonnelly-zspace/accessibility-standards.git"
  },
  "bugs": {
    "url": "https://github.com/jdonnelly-zspace/accessibility-standards/issues"
  },
  "homepage": "https://github.com/jdonnelly-zspace/accessibility-standards#readme",
  "dependencies": {
    "axios": "^1.12.2",
    "cheerio": "^1.0.0",
    "node-html-markdown": "^1.3.0"
  },
  "devDependencies": {}
}
