{
  "name": "accessibility-standards-unity",
  "version": "3.1.0",
  "description": "Comprehensive accessibility standards framework for zSpace Unity applications - WCAG 2.2 Level AA + W3C XAUR compliance with automated auditing, screenshot capture, visual analysis, compliance tracking, CI/CD integration, and code generation for stereoscopic 3D",
  "main": "index.js",
  "type": "module",
  "bin": {
    "a11y-setup": "./bin/setup.js",
    "a11y-audit-zspace": "./bin/audit.js"
  },
  "scripts": {
    "setup": "node bin/setup.js",
    "audit": "node bin/audit.js",
    "test": "echo \"No tests yet\" && exit 0",
    "prepublishOnly": "echo 'Preparing for publish...' && node -v"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "bin/",
    "templates/",
    "standards/",
    "implementation/",
    "workflows/",
    "docs/",
    "resources/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "package.json"
  ],
  "keywords": [
    "accessibility",
    "unity",
    "unity3d",
    "zspace",
    "stereoscopic-3d",
    "stereoscopic",
    "depth-perception",
    "stylus-input",
    "wcag",
    "wcag-2.2",
    "xaur",
    "w3c",
    "a11y",
    "vpat",
    "level-aa",
    "spatial-computing",
    "game-accessibility",
    "education",
    "medical-training",
    "desktop-vr",
    "screen-reader",
    "nvda",
    "keyboard-accessibility",
    "screenshot",
    "visual-analysis",
    "contrast-checker",
    "color-blind"
  ],
  "author": "jPdonnelly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdonnelly-zspace/accessibility-standards-unity.git"
  },
  "bugs": {
    "url": "https://github.com/jdonnelly-zspace/accessibility-standards-unity/issues"
  },
  "homepage": "https://github.com/jdonnelly-zspace/accessibility-standards-unity#readme",
  "dependencies": {
    "axios": "^1.12.2",
    "cheerio": "^1.0.0",
    "node-html-markdown": "^1.3.0",
    "sharp": "^0.33.0",
    "color-contrast-checker": "^2.1.0",
    "puppeteer": "^23.0.0",
    "marked": "^15.0.0",
    "csv-writer": "^1.6.0",
    "@octokit/rest": "^21.0.0",
    "tesseract.js": "^5.0.4",
    "natural": "^7.0.7",
    "fluent-ffmpeg": "^2.1.3",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "glob": "^10.3.10",
    "yaml": "^2.3.4"
  },
  "devDependencies": {}
}
